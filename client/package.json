{
    "name": "angular-material",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "dev": "ng serve -o",
        "dev:websocket": "ng serve -o --configuration websocket",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "clean": "rm -rf .angular node_modules package-lock.json",
        "ts-check": "tsc --noEmit",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "format": "prettier --write \"src/**/*.{ts,html,scss,css}\"",
        "format:check": "prettier --check \"src/**/*.{ts,html,scss,css}\""
    },
    "dependencies": {
        "@angular/animations": "^20.3.4",
        "@angular/cdk": "^20.2.8",
        "@angular/common": "^20.3.4",
        "@angular/compiler": "^20.3.4",
        "@angular/core": "^20.3.4",
        "@angular/forms": "^20.3.4",
        "@angular/material": "^20.2.8",
        "@angular/platform-browser": "^20.1.3",
        "@angular/router": "^20.3.4",
        "@ngneat/until-destroy": "^10.0.0",
        "@ngrx/effects": "^20.0.0",
        "@ngrx/store": "^20.0.0",
        "dayjs": "^1.11.13",
        "rxjs": "~7.8.2",
        "socket.io-client": "^4.8.1",
        "tslib": "^2.8.1",
        "zone.js": "~0.15.1"
    },
    "devDependencies": {
        "@angular-eslint/eslint-plugin": "^20.1.1",
        "@angular-eslint/eslint-plugin-template": "^20.1.1",
        "@angular-eslint/template-parser": "^20.1.1",
        "@angular/build": "^20.3.5",
        "@angular/cli": "^20.3.5",
        "@angular/compiler-cli": "^20.3.4",
        "@eslint/js": "^9.32.0",
        "@types/jasmine": "~5.1.8",
        "@types/lodash": "^4.17.20",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "eslint": "^9.32.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.3",
        "globals": "^16.3.0",
        "jasmine-core": "~5.9.0",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "prettier": "^3.6.2",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.38.0"
    }
}
